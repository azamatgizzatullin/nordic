{
    "source": {
      "include": [
        "./modules/user/handlers.js",
        "./modules/good/handlers.js"
      ],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "./docs/",
      "readme": "./README.md"
    }
  }